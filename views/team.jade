extends layout

block content
  .mangrove-members
    each member in members
      li=member.name

  .mangrove-map
    #map-container

  script(src="https://developers.google.com/maps/documentation/javascript/examples/markerclusterer/markerclusterer.js")
  script(src="https://maps.googleapis.com/maps/api/js?key=AIzaSyAL_vRgjm7TU4HjzwIPWe4XDTwH0DcEKHY")
  script(src="scripts/team.js")
  script(src="scripts/livemap.js")
  script.
    $(function() {
      // Map section
      var optionsMap = {
        elements: !{JSON.stringify(coords)},
        mapDiv: 'map-container'
      };

      var mapSection = new window.MapSection(optionsMap);
      mapSection.init();
    });
